var sogUtils = {
  productCardTemplate: document.querySelector('[data-product-card-template]').innerHTML,
  noImageUrl: '{{ 'no-img.png' | asset_url }}',

  /**
   * @param {Object[]} products - An array of Shopify product objects.
   * @param {string} containerSelector - The element where to insert the generated HTML.
   */
  renderProductGrid: function(products, containerSelector) {
    const container = document.querySelector(containerSelector);

    if (container === null) { return; }
    if (!Array.isArray(products) || !products.length) { return; }

    let items = '';
    products.forEach(function(product) {
      let item = sogUtils.productCardTemplate;

      item = item.replace(/{url}/g, '/products/' + product.handle);
      item = item.replace(/{title}/g, product.title);
      // item = item.replace(/{price_min}/g, Shopify.formatMoney(product.variants.reduce(function(v, p) { let x = parseFloat(p.price); return x < v ? x : v }).price, Shopify.money_format));
      // item = item.replace(/{price_min}/g, Shopify.formatMoney(product.variants[0].price, Shopify.money_format));

      // Search and collection endpoints return different formats
      item = item.replace(/{price_min}/g, Shopify.formatMoney(Array.isArray(product.variants) && product.variants.length ? product.variants[0].price : product.price * 100 , Shopify.money_format));

      // Search and collection endpoints return different formats
      let imageUrl1 = sogUtils.noImageUrl;
      let imageUrl2;
      if (Array.isArray(product.images) && product.images.length) {
        imageUrl1 = product.images[0].src;
        imageUrl2 = product.images[1] !== undefined ? product.images[1].src : imageUrl1;
      } else if (product.image1 !== undefined) {
        imageUrl1 = product.image1.src;
        imageUrl2 = product.image2 !== null ? product.image2.src : imageUrl1;
      }

      [
        [ /{image1_1x}/g, '_180x180$1',     /{image1_1x_hover}/g, '_180x180$1'    ],
        [ /{image1_2x}/g, '_180x180@2x$1',  /{image1_2x_hover}/g, '_180x180@2x$1' ],
        [ /{image2_1x}/g, '_250x250$1',     /{image2_1x_hover}/g, '_250x250$1'    ],
        [ /{image2_2x}/g, '_250x250@2x$1',  /{image2_2x_hover}/g, '_250x250@2x$1' ],
        [ /{image3_1x}/g, '_330x330$1',     /{image3_1x_hover}/g, '_330x330$1'    ],
        [ /{image3_2x}/g, '_330x330@2x$1',  /{image3_2x_hover}/g, '_330x330@2x$1' ],
        [ /{image4_1x}/g, '_500x500$1',     /{image4_1x_hover}/g, '_500x500$1'    ],
        [ /{image4_2x}/g, '_500x500@2x$1',  /{image4_2x_hover}/g, '_500x500@2x$1' ]
      ].forEach(function(sizes) {
        item = item.replace(sizes[0], imageUrl1.replace(/(\.[^.]*)$/, sizes[1]));
        item = item.replace(sizes[2], imageUrl2.replace(/(\.[^.]*)$/, sizes[3]));
      });

      items += item;
    });

    container.insertAdjacentHTML('beforeend', items);
  }

};

// Tabbed featured collections
(function($, undefined) {
  'use strict';

  $(document).ready(function() {

    // Tabbed Panels
    // -------------------------------------------------------------------------
    var tabbedPanelsSelectors = {
      tab: '[data-tabbed-panels-tab]',
      panel: '[data-tabbed-panels-panel]',
      tabActive: '.nav-tabs__item__link--active',
      panelActive: '.tabbed-featured-products__panels__panel--active'
    };

    var tabbedPanelsClasses = {
      tabActive: 'nav-tabs__item__link--active',
      panelActive: 'tabbed-featured-products__panels__panel--active'
    };

    $(tabbedPanelsSelectors.tab).on('click', function(event) {
      event.preventDefault();

      $(tabbedPanelsSelectors.tabActive).removeClass(tabbedPanelsClasses.tabActive);
      $(this).addClass(tabbedPanelsClasses.tabActive);

      $(tabbedPanelsSelectors.panelActive).removeClass(tabbedPanelsClasses.panelActive);
      $($(this).attr('href')).addClass(tabbedPanelsClasses.panelActive);
    });

  });

})(jQuery);
