	getResults: function(page, limit) {
		var tags = (((typeof window.current_tags != "undefined") && window.current_tags != "") ? window.current_tags: "");
		$.ajax({
			method: "GET",
			url: "https://search.sonsofgotham.com/collection",
			data: { id: window.collection.id, page: page, per_page: 24, tags: tags, sort_by: $('#sort-by').val() },
			success: function(result) {
				if (result.data.length > 0) {
					window.collection_products = result.data;
					window.collection_page = result.page;
					window.collection_more = result.has_more;
					collectionPage.init(window.collection_products);
				}
				$collectionLoading.hide();
				window.collection_loading = false;
			}
		});
	}
